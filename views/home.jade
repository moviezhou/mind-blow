extends layout

block content
  #wrapper
    #logo
        img(src='/images/logo.png')
    #display
        h1= title
        p Welcome to #{title}
        #login
            form(method='post')
                | Enter youre name if you want to be a bloger
                div
                    input(type='text', name='username')
                    input(type='submit', value='Log In')
    include footer